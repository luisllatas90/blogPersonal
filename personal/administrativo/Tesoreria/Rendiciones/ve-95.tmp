                    <asp:Button ID="cmdfinalizar" runat="server" BackColor="LemonChiffon" Font-Bold="False" Text="   Dar por finalizada"
                        Width="160px" Font-Names="Verdana" Font-Size="8pt" Height="24px" style="background-image: url(iconos/guardar.gif); background-repeat: no-repeat" BorderStyle="Groove" />
                    <input id="cmdagregar" style="width: 112px; background-repeat: no-repeat; background-color: lemonchiffon"
